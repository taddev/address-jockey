{
    "name": "taddev/address-jockey",
    "description": "Simple Address List Management.",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Tad DeVries",
            "email": "taddevries@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "slim/slim": "^3.1",
        "slim/php-view": "^2.0",
        "monolog/monolog": "^1.17",
        "robmorgan/phinx": "^0.6.4",
        "vlucas/phpdotenv": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0",
        "fzaninotto/faker": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "AddressJockey\\": "src/app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "php -S 0.0.0.0:8080 -t public public/index.php",
        "test": "phpunit"
    }
}
